      <%= image_tag("deja_vu.jpg", class:"card-img-bottom") %>
    </div>

    <div class="card works-card" data-bs-toggle="modal" data-bs-target="#watchlistModal">
      <div class="card-body">
        <h5 class="card-title"> Watchlist</h5>
      </div>
      <%= image_tag("watchlist.png", class:"card-img-bottom") %>
    </div>

    <div class="card works-card" data-bs-toggle="modal" data-bs-target="#quizzleModal">
